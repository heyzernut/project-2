OUR ROUTE PLANS

app.use('/')
  GET /
  GET /search
  POST /search

app.use('login')
  GET /
  POST /

app.use('/register')
  GET /
  POST /

app.use('/destination')
GET /
  POST /

app.use('/routes')
  GET /new
  GET /:id
  POST /
  POST /:id
  DELETE /:id

app.use('/admins)
  GET /register
  POST /register
